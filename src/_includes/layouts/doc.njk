---
layout: layouts/default.njk
---

{% if hasTabs %}
    <script type="module" src="/js/seven-minute-tabs.js" async></script>
{% endif %}
{{ content | safe }}
{% set nextPage = collections.all | getDocumentsFromCollection(repo) | eleventyNavigation | nextPage(page.url) %}
{% set previousPage = collections.all | getDocumentsFromCollection(repo) | eleventyNavigation | previousPage(page.url) %}
<div class="not-prose">
    {% if previousPage or nextPage %}
        <div class="bg-base-content/10 my-10 mx-1 h-px"></div>
        <div class="flex justify-between">
            <div>
                {% if previousPage %}
                    <a href="{{ previousPage.url }}" class="btn btn-sm md:btn-md btn-ghost gap-2 normal-case lg:gap-3">
                        <svg
                            class="h-6 w-6 fill-current md:h-8 md:w-8"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24">
                            <path d="M15.41,16.58L10.83,12L15.41,7.41L14,6L8,12L14,18L15.41,16.58Z"></path>
                        </svg>
                        <div class="flex flex-col items-start">
                            <span class="text-base-content/50 hidden text-xs font-normal md:block">Prev</span>
                            <span>{{ previousPage.title }}</span>
                        </div>
                    </a>
                {% endif %}
            </div>
            <div>
                {% if nextPage %}
                    <a href="{{ nextPage.url }}" class="btn btn-sm btn-ghost md:btn-md gap-2 normal-case lg:gap-3">
                        <div class="flex flex-col items-end">
                            <span class="text-primary-content/50 hidden text-xs font-normal md:block">Next</span>
                            <span>{{ nextPage.title }}</span>
                        </div>
                        <svg
                            class="h-6 w-6 fill-current md:h-8 md:w-8"
                            xmlns="http://www.w3.org/2000/svg"
                            width="24"
                            height="24"
                            viewBox="0 0 24 24">
                            <path d="M8.59,16.58L13.17,12L8.59,7.41L10,6L16,12L10,18L8.59,16.58Z"></path>
                        </svg>
                    </a>
                {% endif %}
            </div>
        </div>
    {% endif %}
    <div class="bg-base-content/10 my-10 mx-1 h-px"></div>
    <div class="flex flex-col justify-between gap-2 px-4 text-xs md:flex-row">
        <div class="flex flex-col gap-2">
            {% if not hideChangelog %}
                <div class="flex items-center gap-2">
                    <svg
                        xmlns="http://www.w3.org/2000/svg"
                        class="inline-block h-4 w-4 fill-current"
                        width="24"
                        height="24"
                        viewBox="0 0 24 24"
                        stroke-width="1.5"
                        stroke="#2c3e50"
                        fill="none"
                        stroke-linecap="round"
                        stroke-linejoin="round">
                        <path stroke="none" d="M0 0h24v24H0z" fill="none"/>
                        <circle cx="7" cy="18" r="2"/>
                        <circle cx="7" cy="6" r="2"/>
                        <circle cx="17" cy="12" r="2"/>
                        <line x1="7" y1="8" x2="7" y2="16"/>
                        <path d="M7 8a4 4 0 0 0 4 4h4"/>
                    </svg>
                    <div>
                        View the {{ name }}
                        <a target="_blank" rel="noopener" class="link" href="{{ repository }}/blob/main/CHANGELOG.md">changelog</a>
                    </div>
                </div>
            {% endif %}
            <div class="flex items-center gap-2">
                <svg
                    class="inline-block h-4 w-4 fill-current"
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 1792 1792">
                    <path d="M1696 960q0 26-19 45t-45 19h-224q0 171-67 290l208 209q19 19 19 45t-19 45q-18 19-45 19t-45-19l-198-197q-5 5-15 13t-42
                        28.5-65 36.5-82 29-97 13v-896h-128v896q-51 0-101.5-13.5t-87-33-66-39-43.5-32.5l-15-14-183 207q-20 21-48 21-24
                        0-43-16-19-18-20.5-44.5t15.5-46.5l202-227q-58-114-58-274h-224q-26 0-45-19t-19-45 19-45
                        45-19h224v-294l-173-173q-19-19-19-45t19-45 45-19 45 19l173 173h844l173-173q19-19 45-19t45 19 19 45-19 45l-173
                        173v294h224q26 0 45 19t19 45zm-480-576h-640q0-133 93.5-226.5t226.5-93.5 226.5 93.5 93.5 226.5z"></path>
                </svg>
                <div>Do you see a bug?
                    <a target="_blank" rel="noopener" class="link" href="{{ repository }}/issues/">Open an issue on
                                                GitHub</a>
                </div>
            </div>
            <div class="flex items-center gap-2">
                <svg
                    class="inline-block h-4 w-4 fill-current"
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 512 512">
                    <path d="M256,448a32,32,0,0,1-18-5.57c-78.59-53.35-112.62-89.93-131.39-112.8-40-48.75-59.15-98.8-58.61-153C48.63,114.52,98.46,64,159.08,64c44.08,0,74.61,24.83,92.39,45.51a6,6,0,0,0,9.06,0C278.31,88.81,308.84,64,352.92,64,413.54,64,463.37,114.52,464,176.64c.54,54.21-18.63,104.26-58.61,153-18.77,22.87-52.8,59.45-131.39,112.8A32,32,0,0,1,256,448Z"></path>
                </svg>
                <div>Do you like {{ name }}?
                    <a target="_blank" rel="noopener" class="link" href="{{ repository }}">
                        Star it on GitHub!</a>
                </div>
            </div>
        </div>
        <div class="flex flex-col gap-2">
            <div class="flex items-center gap-2">
                <svg
                    class="inline-block h-4 w-4 fill-current"
                    xmlns="http://www.w3.org/2000/svg"
                    width="24"
                    height="24"
                    viewBox="0 0 24 24">
                    <path d="M20.71,7.04C21.1,6.65 21.1,6 20.71,5.63L18.37,3.29C18,2.9 17.35,2.9
                        16.96,3.29L15.12,5.12L18.87,8.87M3,17.25V21H6.75L17.81,9.93L14.06,6.18L3,17.25Z"></path>
                </svg>
                <div>
                    <a
                        target="_blank"
                        rel="noopener"
                        class="link"
                        href="https://github.com/thegreenwebfoundation/developer-docs/blob/main/{{ page.inputPath }}">Edit this page on GitHub</a>
                </div>
            </div>
        </div>
    </div>
</div>